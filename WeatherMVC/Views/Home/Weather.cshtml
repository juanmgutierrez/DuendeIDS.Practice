@model List<Weather>

@{
    ViewData["Title"] = "Home Page";
    Layout = "_Layout";
}

<h2>Weather</h2>

<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Date</td>
            <td>@item.TemperatureC</td>
            <td>@item.Summary</td>
        </tr>
    }
</table>